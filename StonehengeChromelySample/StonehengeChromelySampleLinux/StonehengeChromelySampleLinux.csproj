<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <RuntimeIdentifiers>linux-x64</RuntimeIdentifiers>
        <TargetFrameworks>netcoreapp2.1</TargetFrameworks>
        <AssemblyName>StonehengeChromelySample</AssemblyName>
        <RootNamespace>StonehengeChromelySample</RootNamespace>
        <AssemblyVersion>1.0.1</AssemblyVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Chromely.CefGlue.Gtk" Version="68.0.0-beta02" />
      <PackageReference Include="IctBaden.Stonehenge3" Version="3.3.5" />
      <PackageReference Include="SharpZipLib" Version="1.1.0" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="app\start.html" />
      <None Remove="stonehenge-chromely.ico" />
      <Content Include="stonehenge-chromely.ico">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
      <EmbeddedResource Include="app\start.html" />
    </ItemGroup>

</Project>
